{
  "$schema": "https://json.schemastore.org/launchsettings.json",
  "iisSettings": {
    "windowsAuthentication": false,
    "anonymousAuthentication": true,
    "iisExpress": {
      "applicationUrl": "http://localhost:42471",
      "sslPort": 44319
    }
  },
  "profiles": {
    "ZiraLink.Api": {
      "commandName": "Project",
      "dotnetRunMessages": true,
      "launchBrowser": true,
      "launchUrl": "swagger",
      "applicationUrl": "http://api.ziralink.local:6002",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development",
        "ZIRALINK_CONNECTIONSTRINGS_DB": "Data Source=database.db",
        "ZIRALINK_CONNECTIONSTRINGS_RABBITMQ": "amqp://user:Pass123$@localhost:5772/",
        "ZIRALINK_CONNECTIONSTRINGS_REDIS": "localhost:6479",
        "ZIRALINK_REDIS_PASSWORD": "",
        "ZIRALINK_URL_IDS": "http://ids.ziralink.local:5002",
        "ZIRALINK_REDIRECTURI": "http://ziralink.local:4002/signin-result",
        "ZIRALINK_WEB_URL": "http://ziralink.local:4002",
        "ZIRALINK_USE_HTTP": "true",
        "ASPNETCORE_Kestrel__Certificates__Default__Password": "zira_cert",
        "ASPNETCORE_Kestrel__Certificates__Default__Path": "server.pfx"
      }
    },
    "IIS Express": {
      "commandName": "IISExpress",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      }
    }
  }
}
